{
  "printWidth": 100,
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@components/",
    "^@hooks/",
    "^@store/",
    "^@utils/",
    "^@api/",
    "^@constants/",
    "^@styles/",
    "^@images/",
    "^@icons/",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
